<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gl_v_4.44"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.0444"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gl_v_79.2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.792"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gl_v_95.56"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.9556"/>

    <!-- Tool bar -->
    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:contentInsetStartWithNavigation="0dp"
        android:background="@color/white"
        app:titleTextColor="@color/black"
        style="@style/ToolbarStyle.Dark"
        />
    <!-- -->

    <View
        android:id="@+id/view_1"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/toolbar"
        app:layout_constraintDimensionRatio="360:32"/>

    <View
        android:id="@+id/view_2"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/view_1"
        app:layout_constraintDimensionRatio="360:56"/>


    <TextView
        android:id="@+id/tv_question_des"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/view_1"
        app:layout_constraintBottom_toBottomOf="@id/view_2"
        app:layout_constraintLeft_toLeftOf="@id/gl_v_4.44"
        app:layout_constraintRight_toRightOf="@id/gl_v_95.56"
        android:maxLines="2"
        style="@style/tv_withdrawal_1"/>

    <View
        android:id="@+id/view_3"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/tv_question_des"
        app:layout_constraintDimensionRatio="360:24"/>

    <View
        android:id="@+id/view_4"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/view_3"
        app:layout_constraintDimensionRatio="360:20"/>

    <View
        android:id="@+id/view_5"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/view_4"
        app:layout_constraintDimensionRatio="360:20"/>

    <View
        android:id="@+id/view_point_1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintLeft_toLeftOf="@id/gl_v_4.44"
        app:layout_constraintTop_toTopOf="@id/view_4"
        app:layout_constraintBottom_toBottomOf="@id/view_4"
        app:layout_constraintWidth_percent="0.011"
        app:layout_constraintDimensionRatio="1:1"
        android:background="@drawable/point_black"/>

    <View
        android:id="@+id/view_1_1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintLeft_toRightOf="@id/view_point_1"
        app:layout_constraintWidth_percent="0.02"
        app:layout_constraintTop_toTopOf="@id/view_point_1"
        app:layout_constraintBottom_toBottomOf="@id/view_point_1"/>

    <TextView
        android:id="@+id/tv_des_1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintLeft_toRightOf="@id/view_1_1"
        app:layout_constraintRight_toRightOf="@id/gl_v_95.56"
        app:layout_constraintTop_toTopOf="@id/view_4"
        app:layout_constraintBottom_toBottomOf="@id/view_5"

        android:text="@string/tv_withdrawal_mentor_1"
        style="@style/tv_withdrawal_2"/>

    <View
        android:id="@+id/view_6"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/tv_des_1"
        app:layout_constraintDimensionRatio="360:8"/>


    <View
        android:id="@+id/view_7"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/view_6"
        app:layout_constraintDimensionRatio="360:20"/>

    <View
        android:id="@+id/view_8"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/view_7"
        app:layout_constraintDimensionRatio="360:20"/>


    <View
        android:id="@+id/view_point_2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintLeft_toLeftOf="@id/gl_v_4.44"
        app:layout_constraintTop_toTopOf="@id/view_7"
        app:layout_constraintBottom_toBottomOf="@id/view_7"
        app:layout_constraintWidth_percent="0.011"
        app:layout_constraintDimensionRatio="1:1"
        android:background="@drawable/point_black"/>

    <View
        android:id="@+id/view_1_2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintLeft_toRightOf="@id/view_point_2"
        app:layout_constraintWidth_percent="0.02"
        app:layout_constraintTop_toTopOf="@id/view_point_2"
        app:layout_constraintBottom_toBottomOf="@id/view_point_2"/>


    <TextView
        android:id="@+id/tv_des_2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintLeft_toRightOf="@id/view_1_2"
        app:layout_constraintRight_toRightOf="@id/gl_v_95.56"
        app:layout_constraintTop_toTopOf="@id/view_7"
        app:layout_constraintBottom_toBottomOf="@id/view_8"

        android:text="@string/tv_withdrawal_mentor_mentee"
        style="@style/tv_withdrawal_2"/>


    <View
        android:id="@+id/view_9"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/tv_des_2"
        app:layout_constraintDimensionRatio="360:55"/>

    <ImageView
        android:id="@+id/iv_check"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintLeft_toLeftOf="@id/gl_v_4.44"
        app:layout_constraintTop_toBottomOf="@id/view_9"
        app:layout_constraintWidth_percent="0.066"
        app:layout_constraintDimensionRatio="1:1"
        android:src="@drawable/ic_checkbox_off"
        />

    <View
        android:id="@+id/view_10"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="@id/iv_check"
        app:layout_constraintBottom_toBottomOf="@id/iv_check"
        app:layout_constraintLeft_toRightOf="@id/iv_check"
        app:layout_constraintWidth_percent="0.02"
        />

    <TextView
        android:id="@+id/tv_check_terms"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="@id/view_10"
        app:layout_constraintBottom_toBottomOf="@id/view_10"
        app:layout_constraintLeft_toRightOf="@id/view_10"
        app:layout_constraintRight_toRightOf="@id/gl_v_79.2"
        app:layout_constraintDimensionRatio="235:22"
        style="@style/tv_withdrawal_2"

        android:text="@string/tv_check_withdrawal"
        android:maxLines="1"
        />




    <View
        android:id="@+id/view_for_btn_1"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintDimensionRatio="360:24"/>

    <View
        android:id="@+id/view_for_btn_2"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@id/view_for_btn_1"
        app:layout_constraintDimensionRatio="360:52"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/btn_withdrawal_active"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@id/view_for_btn_1"
        app:layout_constraintTop_toTopOf="@id/view_for_btn_2"
        app:layout_constraintLeft_toLeftOf="@id/gl_v_4.44"
        app:layout_constraintRight_toRightOf="@id/gl_v_95.56"

        android:text="회원탈퇴"
        android:textColor="@color/white"
        android:backgroundTint="@color/tuktalk_primary"
        app:shapeAppearanceOverlay="@style/buttonRadiusShape2"

        android:visibility="invisible"

        />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/btn_withdrawal_unactive"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@id/view_for_btn_1"
        app:layout_constraintTop_toTopOf="@id/view_for_btn_2"
        app:layout_constraintLeft_toLeftOf="@id/gl_v_4.44"
        app:layout_constraintRight_toRightOf="@id/gl_v_95.56"

        android:text="회원탈퇴"
        android:textColor="@color/tuktalk_sub_content_4"
        android:backgroundTint="@color/tuktalk_gray_4"
        app:shapeAppearanceOverlay="@style/buttonRadiusShape2"

        android:visibility="visible"
        android:enabled="false"
        />



</androidx.constraintlayout.widget.ConstraintLayout>

</layout>